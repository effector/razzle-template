{
  "name": "razzle-examples-with-typescript",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "format": "prettier --write \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "commit": "git-cz",
    "lint": "eslint --fix ./src"
  },
  "engines": {
    "node": ">=14.0.0 <=15.2.0"
  },
  "dependencies": {
    "cross-fetch": "^3.0.6",
    "effector": "^21.5.0",
    "effector-inspector": "^0.4.1",
    "effector-logger": "^0.9.0",
    "effector-react": "^21.0.6",
    "effector-root": "^1.1.0",
    "express": "^4.17.1",
    "patronum": "^0.110.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.0.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@eslint-kit/eslint-config-base": "^2.1.0",
    "@eslint-kit/eslint-config-node": "^2.0.0",
    "@eslint-kit/eslint-config-prettier": "^2.0.0",
    "@eslint-kit/eslint-config-react": "^2.0.0",
    "@eslint-kit/eslint-config-typescript": "^2.0.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^23.3.14",
    "@types/node": "^12.6.6",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-router": "^5.1.8",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^5.1.1",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-plugin-styled-components": "^1.10.7",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^7.4.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "husky": "^3.1.0",
    "jest": "^26.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.1.2",
    "razzle": "^3.3.8",
    "razzle-plugin-typescript": "^3.3.8",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "typescript": "4"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^lib/(.*)$": "<rootDir>/src/lib/$1"
    },
    "transformIgnorePatterns": ["node_modules"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": ["ts", "tsx", "js", "json"],
    "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}"]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
